<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="xyz.bikea.mapper.OrderMapper">

    <select id="queryOrderList" resultType="order">
        select * from order_info limit 100
    </select>

    <select id="queryNumOrder" resultType="int">
        select num_orders from date_record limit 14
    </select>

    <select id="queryIncome" resultType="int">
        select income from date_record limit 14
    </select>

</mapper>